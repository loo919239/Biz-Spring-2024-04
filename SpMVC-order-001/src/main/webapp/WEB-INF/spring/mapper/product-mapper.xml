<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.callor.hello.persistance.ProductDao">

	<update id="update">
	UPDATE tbl_customer
	SET c_name = #{c_name}, c_tel = #{c_tel}
	WHERE c_code = #{c_code}
	</update>
</mapper> 